{
  "name": "christmas-gift-exchange-assignment",
  "image": "mcr.microsoft.com/devcontainers/typescript-node:1-20-bullseye",
  "mounts": [
    {
      "type": "volume",
      "source": "christmas-gift-exchange-assignment-node_modules",
      "target": "${containerWorkspaceFolder}/node_modules"
    }
  ],
  "runArgs": ["--name", "christmas-gift-exchange-assignment"],
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": ["esbenp.prettier-vscode"]
    }
  }
}
